 public static String[] addRoundKey(String[] input1,String[] input2){
    //Initialize OutputArray
        String[] output = new String[4];
    
        //Loop to calculate each new nibble in array
        for (int i = 0 ; i < 4 ; i++) {
            output[i] = addition(input1[i],input2[i]);
            System.out.println(addition(input1[i],input2[i]));
        }
    
    //output
    return output;
    }
